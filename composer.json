{
    "name": "xavrsl/ldap",
    "description": "Add Ldap Search possibilities to Laravel",
    "keywords": ["laravel", "ldap", "search"],
	"license": "MIT",
    "authors": [
        {
            "name": "Xavier Roussel",
            "email": "contact@xavrsl.fr"
        },
        {
            "name": "Weida Lin",
            "email": "weidaalone@163.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "ext-ldap": "*",
        "illuminate/support": "^5.4"
    },
    "autoload": {
